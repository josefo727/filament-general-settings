{
    "name": "josefo727/filament-general-settings",
    "description": "A Filament plugin for managing general settings in a Laravel application",
    "type": "library",
    "version": "2.0.3",
    "license": "MIT",
    "authors": [
        {
            "name": "Josefo727",
            "email": "josefo727@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "illuminate/support": "^11.28|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "nunomaduro/larastan": "^3.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Josefo727\\FilamentGeneralSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Josefo727\\FilamentGeneralSettings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Josefo727\\FilamentGeneralSettings\\FilamentGeneralSettingsServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
