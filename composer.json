{
    "name": "josefo727/filament-general-settings",
    "description": "A Filament plugin for managing general settings in a Laravel application",
    "type": "library",
    "version": "0.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Josefo727",
            "email": "josefo727@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Josefo727\\FilamentGeneralSettings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josefo727\\FilamentGeneralSettings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Josefo727\\FilamentGeneralSettings\\FilamentGeneralSettingsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}